<h1>Listing survey_answers</h1>

<table>
  <tr>
    <th>Survey</th>
    <th>Question</th>
    <th>Answer</th>
  </tr>

<% @survey_answers.each do |survey_answer| %>
  <tr>
    <td><%=h survey_answer.survey_id %></td>
    <td><%=h survey_answer.question_id %></td>
    <td><%=h survey_answer.answer_id %></td>
    <td><%= link_to 'Show', survey_answer %></td>
    <td><%= link_to 'Edit', edit_survey_answer_path(survey_answer) %></td>
    <td><%= link_to 'Destroy', survey_answer, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New survey_answer', new_survey_answer_path %>